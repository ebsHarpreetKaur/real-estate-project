{
  "expo": {
    "name": "Project",
    "slug": "react-native-real-estate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/newlogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/A_SMART_MOVE__2_-removebg-preview.png",
      "resizeMode": "contain",
      "backgroundColor": "#CDFADB"
    },
    "notification": {
      "androidMode": "default",
      "icon": "./assets/newlogo.png"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/newlogo.png",
        "backgroundColor": "#ffffff"
      },
      "useNextNotificationsApi": true,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.ebullientsoft.unifyreactnativerealestateapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Real Estate App to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "777b348b-9cbf-49e8-9642-23c11e417b0b"
      }
    },
    "owner": "harpreet24"
  }
}
