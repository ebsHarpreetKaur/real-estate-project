{
  "expo": {
    "name": "Unify",
    "slug": "unify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash/logo-withoutbg.png",
      "resizeMode": "contain",
      "backgroundColor": "#00538C"
    },
    "notification": {
      "androidMode": "default",
      "icon": "./assets/splash/logo.png"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "bitcode": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash/logo.png",
        "backgroundColor": "#ffffff"
      },
      "useNextNotificationsApi": true,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.BLUETOOTH",
        "android.permission.CAMERA",
        "android.permission.INTERNET",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.RECORD_AUDIO",
        "android.permission.SYSTEM_ALERT_WINDOW",
        "android.permission.WAKE_LOCK"
      ],
      "package": "com.ebullientsoft.unifyreactnativerealestateapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Real Estate App to use your location."
        }
      ],
      [
        "@config-plugins/react-native-webrtc"
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a970345b-c7bc-4048-9827-c4fd56dde0ec"
      }
    },
    "owner": "h7121999"
  }
}